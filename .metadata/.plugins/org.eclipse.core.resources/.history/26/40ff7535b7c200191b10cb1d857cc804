
public class Gerente extends Funcionario { //Gerente herda da classe Funcionario
	
	private int senha;
	
	public boolean autentica(int senha) {
		if(this.senha == senha ) {
			return true;
		} else {
			return false;
			}
		}
	
//	public double getBonificacao() {
//		return this.salario;
	}
	
	
	
}
